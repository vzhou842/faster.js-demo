include layout

block head
	style.
		body {
			margin: 0;
		}

		.box-container {
			width: 100%;
			height: 100%;
			display: flex;
			flex-direction: horizontal;
		}

		.box {
			flex: 1;
		}

		iframe {
			border: none;
			width: 100%;
			height: 100%;
		}

block body
	.box-container
		.box
			iframe(src='/')
		.box
			iframe(src='/?optimized=1')
